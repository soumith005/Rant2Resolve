# Backend Environment Variables
# Copy this to .env in the backend directory and fill in your values

# MongoDB Connection
MONGO_URI=mongodb+srv://USERNAME:PASSWORD@cluster.mongodb.net/Rant2Resolve?retryWrites=true&w=majority

# Server Configuration
NODE_ENV=production
PORT=5000

# Frontend URL (for CORS configuration)
FRONTEND_URL=https://rant2resolve-frontend.onrender.com

# Google Gemini API (for AI features)
GEMINI_API_KEY=your_gemini_api_key_here

# JWT Secret (change this to a strong random string)
JWT_SECRET=your_jwt_secret_key_here
